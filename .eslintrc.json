{
	"env": {
		"browser": true,
		"es2020": true
	},
	"extends": ["plugin:react/recommended", "airbnb", "prettier"],
	"parserOptions": {
		"ecmaFeatures": {
			"jsx": true
		},
		"ecmaVersion": 11,
		"sourceType": "module"
	},
	"plugins": ["react", "react-hooks"],
	"rules": {
		"react/jsx-filename-extension": "off",
		"react/jsx-indent": "off",
		"no-unused-vars": "warn",
		"no-unused-expressions": "off"
	}
}
